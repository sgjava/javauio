<?xml version="1.0" encoding="UTF-8"?>
<actions>
        <action>
            <actionName>run.single.main</actionName>
            <packagings>
                <packaging>*</packaging>
            </packagings>
            <goals>
                <goal>process-classes</goal>
                <goal>org.codehaus.mojo:exec-maven-plugin:3.0.0:exec</goal>
            </goals>
            <properties>
                <exec.vmArgs></exec.vmArgs>
                <exec.classpath>/home/servadmin/NetBeansProjects/javauio/u8g2/target/u8g2-1.0.0-SNAPSHOT-linux64.jar</exec.classpath>
                <exec.appArgs>--type=SDL</exec.appArgs>
                <exec.args>${exec.vmArgs} -classpath ${exec.classpath}:%classpath ${exec.mainClass} ${exec.appArgs}</exec.args>
                <exec.executable>java</exec.executable>
                <exec.mainClass>${packageClassName}</exec.mainClass>
                <exec.classpathScope>${classPathScope}</exec.classpathScope>
            </properties>
        </action>
    </actions>
